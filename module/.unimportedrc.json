{
    "entry": ["src/index.ts"],
    "pathTransforms": {
        "(\\.(?!/jquery/plugin).+)\\.js$": "$1.ts"
    },
    "ignorePatterns": [
        "src/tests/**/*.ts",
        "**/node_modules/**",
        "**/*.test.{js,jsx,ts,tsx}",
        "./dist",
        "src/index.ts",
        "jest.config.ts"
    ],
    "ignoreUnimported": ["src/tests/**/*.ts"],
    "ignoreUnused": [],
    "ignoreUnresolved": [],
    "rootDir": "./src"
}
