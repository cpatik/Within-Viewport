{
    "name": "@patik/within-viewport-demo",
    "version": "3.0.0",
    "private": true,
    "source": "src/index.html",
    "scripts": {
        "start": "yarn clear-parcel-cache && parcel",
        "clear-parcel-cache": "rm -rf ../.parcel-cache",
        "build": "yarn clear-parcel-cache && parcel build --no-scope-hoist --public-url \"/within-viewport\"",
        "typecheck": "tsc --noEmit",
        "code-lint": "eslint --ext .ts --fix ./src/**/*.ts",
        "lint": "yarn typecheck && yarn code-lint && yarn unused-exports",
        "unused-exports": "ts-prune -p ./tsconfig.json"
    },
    "dependencies": {
        "prismjs": "^1.29.0",
        "zustand": "^4.3.2"
    },
    "devDependencies": {
        "@types/jquery": "^3.5.16",
        "@types/prismjs": "^1.26.0",
        "cypress-terminal-report": "^5.0.2",
        "eslint-plugin-prettier": "^4.2.1",
        "parcel": "^2.8.3"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --cache-location .eslintcache --fix"
    },
    "author": {
        "name": "Craig Patik",
        "email": "craig@patik.com",
        "url": "https://patik.com"
    },
    "license": "BSD-3-Clause"
}
